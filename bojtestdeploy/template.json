{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_WIN2016VM_name": {
            "defaultValue": "WIN2016VM",
            "type": "String"
        },
        "networkInterfaces_WIN2016VMNetworkInterface_name": {
            "defaultValue": "WIN2016VMNetworkInterface",
            "type": "String"
        },
        "publicIPAddresses_VMIP01_name": {
            "defaultValue": "VMIP01",
            "type": "String"
        },
        "virtualNetworks_VNet01_name": {
            "defaultValue": "VNet01",
            "type": "String"
        },
        "storageAccounts_gkkhs3aazymd2standardsa_name": {
            "defaultValue": "gkkhs3aazymd2standardsa",
            "type": "String"
        },
        "extensions_Microsoft.Powershell.DSC_name": {
            "defaultValue": "WIN2016VM/Microsoft.Powershell.DSC",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "comments": "Generalized from resource: '/subscriptions/e81fd02b-f607-406c-83d2-670ce0497fee/resourceGroups/eebojtest/providers/Microsoft.Compute/virtualMachines/WIN2016VM'.",
            "type": "Microsoft.Compute/virtualMachines",
            "name": "[parameters('virtualMachines_WIN2016VM_name')]",
            "apiVersion": "2016-04-30-preview",
            "location": "australiasoutheast",
            "tags": {
                "displayName": "VM01"
            },
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_DS2"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "MicrosoftWindowsServer",
                        "offer": "WindowsServer",
                        "sku": "2016-Datacenter",
                        "version": "latest"
                    },
                    "osDisk": {
                        "osType": "Windows",
                        "name": "VMOSDisk",
                        "createOption": "FromImage",
                        "vhd": {
                            "uri": "[concat('https', '://', parameters('storageAccounts_gkkhs3aazymd2standardsa_name'), '.blob.core.windows.net', '/vhds/VMOSDisk.vhd')]"
                        },
                        "caching": "ReadWrite"
                    },
                    "dataDisks": []
                },
                "osProfile": {
                    "computerName": "[parameters('virtualMachines_WIN2016VM_name')]",
                    "adminUsername": "bojadmin",
                    "windowsConfiguration": {
                        "provisionVMAgent": true,
                        "enableAutomaticUpdates": true
                    },
                    "secrets": []
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[resourceId('Microsoft.Network/networkInterfaces', parameters('networkInterfaces_WIN2016VMNetworkInterface_name'))]"
                        }
                    ]
                }
            },
            "dependsOn": [
                "[resourceId('Microsoft.Storage/storageAccounts', parameters('storageAccounts_gkkhs3aazymd2standardsa_name'))]",
                "[resourceId('Microsoft.Network/networkInterfaces', parameters('networkInterfaces_WIN2016VMNetworkInterface_name'))]"
            ]
        },
        {
            "comments": "Generalized from resource: '/subscriptions/e81fd02b-f607-406c-83d2-670ce0497fee/resourceGroups/eebojtest/providers/Microsoft.Network/networkInterfaces/WIN2016VMNetworkInterface'.",
            "type": "Microsoft.Network/networkInterfaces",
            "name": "[parameters('networkInterfaces_WIN2016VMNetworkInterface_name')]",
            "apiVersion": "2016-03-30",
            "location": "australiasoutheast",
            "tags": {
                "displayName": "VMNic01"
            },
            "properties": {
                "ipConfigurations": [
                    {
                        "name": "ipconfig1",
                        "properties": {
                            "privateIPAddress": "10.0.0.4",
                            "privateIPAllocationMethod": "Dynamic",
                            "publicIPAddress": {
                                "id": "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddresses_VMIP01_name'))]"
                            },
                            "subnet": {
                                "id": "[concat(resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_VNet01_name')), '/subnets/Subnet-1')]"
                            }
                        }
                    }
                ],
                "dnsSettings": {
                    "dnsServers": []
                },
                "enableIPForwarding": false
            },
            "dependsOn": [
                "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddresses_VMIP01_name'))]",
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_VNet01_name'))]"
            ]
        },
        {
            "comments": "Generalized from resource: '/subscriptions/e81fd02b-f607-406c-83d2-670ce0497fee/resourceGroups/eebojtest/providers/Microsoft.Network/publicIPAddresses/VMIP01'.",
            "type": "Microsoft.Network/publicIPAddresses",
            "name": "[parameters('publicIPAddresses_VMIP01_name')]",
            "apiVersion": "2016-03-30",
            "location": "australiasoutheast",
            "tags": {
                "displayName": "VMIP01"
            },
            "properties": {
                "publicIPAllocationMethod": "Dynamic",
                "idleTimeoutInMinutes": 4,
                "dnsSettings": {
                    "domainNameLabel": "eebojtest"
                }
            },
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/e81fd02b-f607-406c-83d2-670ce0497fee/resourceGroups/eebojtest/providers/Microsoft.Network/virtualNetworks/VNet01'.",
            "type": "Microsoft.Network/virtualNetworks",
            "name": "[parameters('virtualNetworks_VNet01_name')]",
            "apiVersion": "2016-03-30",
            "location": "australiasoutheast",
            "tags": {
                "displayName": "VNet01"
            },
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "10.0.0.0/16"
                    ]
                },
                "subnets": [
                    {
                        "name": "Subnet-1",
                        "properties": {
                            "addressPrefix": "10.0.0.0/24"
                        }
                    }
                ]
            },
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/e81fd02b-f607-406c-83d2-670ce0497fee/resourceGroups/eebojtest/providers/Microsoft.Storage/storageAccounts/gkkhs3aazymd2standardsa'.",
            "type": "Microsoft.Storage/storageAccounts",
            "sku": {
                "name": "Premium_LRS",
                "tier": "Premium"
            },
            "kind": "Storage",
            "name": "[parameters('storageAccounts_gkkhs3aazymd2standardsa_name')]",
            "apiVersion": "2016-01-01",
            "location": "australiasoutheast",
            "tags": {
                "displayName": "Storage01"
            },
            "properties": {},
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/e81fd02b-f607-406c-83d2-670ce0497fee/resourceGroups/eebojtest/providers/Microsoft.Compute/virtualMachines/WIN2016VM/extensions/Microsoft.Powershell.DSC'.",
            "type": "Microsoft.Compute/virtualMachines/extensions",
            "name": "[parameters('extensions_Microsoft.Powershell.DSC_name')]",
            "apiVersion": "2016-04-30-preview",
            "location": "australiasoutheast",
            "tags": {
                "AutomationAccountARMID": "subscriptions/e81fd02b-f607-406c-83d2-670ce0497fee/resourceGroups/EEAutomationRG/providers/Microsoft.Automation/automationAccounts/EEAutomation"
            },
            "properties": {
                "publisher": "Microsoft.Powershell",
                "type": "DSC",
                "typeHandlerVersion": "2.22",
                "autoUpgradeMinorVersion": false,
                "settings": {
                    "ModulesUrl": "https://eus2oaasibizamarketprod1.blob.core.windows.net/automationdscpreview/RegistrationMetaConfigV2.zip",
                    "ConfigurationFunction": "RegistrationMetaConfigV2.ps1\\RegistrationMetaConfigV2",
                    "Properties": [
                        {
                            "Name": "RegistrationKey",
                            "Value": {
                                "UserName": "PLACEHOLDER_DONOTUSE",
                                "Password": "PrivateSettingsRef:registrationKeyPrivate"
                            },
                            "TypeName": "System.Management.Automation.PSCredential"
                        },
                        {
                            "Name": "RegistrationUrl",
                            "Value": "https://ase-agentservice-prod-1.azure-automation.net/accounts/a2df28ae-1e78-4688-8bd4-3b295718117f",
                            "TypeName": "System.String"
                        },
                        {
                            "Name": "NodeConfigurationName",
                            "Value": "EEInstalliiS.localhost",
                            "TypeName": "System.String"
                        },
                        {
                            "Name": "ConfigurationMode",
                            "Value": "applyAndMonitor",
                            "TypeName": "System.String"
                        },
                        {
                            "Name": "ConfigurationModeFrequencyMins",
                            "Value": "15",
                            "TypeName": "System.Int32"
                        },
                        {
                            "Name": "RefreshFrequencyMins",
                            "Value": "30",
                            "TypeName": "System.Int32"
                        },
                        {
